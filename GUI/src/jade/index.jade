html
    head
        title This is title
        link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.1/css/foundation.min.css')
        link(rel='stylesheet', href='css/index.css')
    body
        .row
            h1 CS423 - Operating Systems Design MP4
            #local.small-12.large-6.column
                h3 Local
                ul
                    li # job:
                        span.job 0
                    li # job finished:
                        span.finished 0
                    li Throttling:
                        span.throttling 0
                    li # job transfered:
                        span.jobTransfered 0
                    li Delay:
                        span.delay 0
                    li Bandwidth:
                        span.bandwidth 0
                p New Local Throttling:
                    input(type='text', placeholder='please enter a floating point number between 0 and 1')
                button(type='button', onclick='updateThrottling("local");') Submit
            #remote.small-12.large-6.column
                h3 Remote
                ul
                    li # job:
                        span.job 0
                    li # job finished:
                        span.finished 0
                    li Throttling:
                        span.throttling 0
                    li # job transfered:
                        span.jobTransfered 0
                    li Delay:
                        span.delay 0
                    li Bandwidth:
                        span.bandwidth 0
                p New Remote Throttling:
                    input(type='text', placeholder='please enter a floating point number between 0 and 1')
                button(type='button', onclick='updateThrottling("remote");') Submit
        script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
        script(src='js/index.js')
